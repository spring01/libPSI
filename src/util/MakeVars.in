
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#

target = @target@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
incdir = @includedir@
docdir = @docdir@
manext = 1
mandir = @mandir@
htmldir = @htmldir@
txtdir = @txtdir@
datadir = @datadir@
datarootdir = @datarootdir@
pkgdatadir=@pkgdatadir@
top_srcdir = @top_srcdir@
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../../..
objincdir = @objincdir@

PWD = $(shell pwd)
RANLIB = @RANLIB@
YACC = @YACC@
CC = @CC@
CXX = @CXX@
FC = @FC@
LD = $(CXX)
MKDIRS = @MKDIRS@
INSTALL = @INSTALL@
INSTALL_INCLUDE = @INSTALL_INCLUDE@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_PROGRAM = @INSTALL_PROGRAM@

PSICPPFLAGS = @PSICPPFLAGS@
CPPFLAGS = @CPPFLAGS@

CDEF = @CDEF@
CDEF += -DUSE_LIBCHKPT
COPT = @COPT@
COTH = @COTH@
CDBG = @CDBG@
CINC = @CINC@

CXXDEF = @CXXDEF@      
CXXDEF += -DUSE_LIBCHKPT 
CXXOPT = @CXXOPT@
CXXOTH = @CXXOTH@
CXXDBG = @CXXDBG@
CXXINC = @CXXINC@

FDEF = @FDEF@
FOPT = @FOPT@
FOTH = @FOTH@
FDBG = @FDBG@
FINC = @FINC@
FSTATIC = @FSTATIC@

# The suffix generated by the -M compiler option
CCDEPENDSUF = @CCDEPENDSUF@
CXXDEPENDSUF = @CXXDEPENDSUF@
CCDEPENDFLAGS = @CCDEPENDFLAGS@
CXXDEPENDFLAGS = @CXXDEPENDFLAGS@
CCDEPEND = $(CC)
CXXDEPEND = $(CXX)

# flags to be passed to compilers
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@
FFLAGS = @FFLAGS@
FFLAGS_NOSTATIC = $(FOPT) $(FOTH) $(FDBG)

# set up suffixes needed for various files
F77SUF = @F77SUF@
OBJSUF = @OBJSUF@
LIBSUF = @LIBSUF@

FLIBS = @FLIBS@
CLIBS = @CLIBS@
CXXLIBS = @CXXLIBS@
BLAS = @BLAS@ $(FLIBS)
LAPACK = @LAPACK@ $(BLAS)

# where to find main for fortran programs
ALLOC = allocate.o
vpath allocate.o $(top_objdir)/lib:$(libdir)

# we are mostly compiling C programs here
LDFLAGS = @LDFLAGS@
LDLIBS = $(CLIBS) @LDLIBS@

# these are the most commonly used libraries
PSILIBS = libipv1.a libciomr.a

vpath %.a $(top_objdir)/lib:$(libdir)

#
# have target default to the name of the directory we're in
#
PSITARGET = $(shell basename `pwd`)

# Force -lx to only look for libx.a.
.LIBPATTERNS = lib%.a

#
# Initialize other variables.
#
.LIBPATTERNS = lib%.a

