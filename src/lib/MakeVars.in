
SHELL = /bin/sh

#
# Initialize variables that have been figured out by configure.
#

target = @target@

prefix = @prefix@
exec_prefix = @exec_prefix@
bindir = @bindir@
libdir = @libdir@
incdir = @includedir@
docdir = @docdir@
manext = 5
mandir = @mandir@
htmldir = @htmldir@
txtdir = @txtdir@
datadir = @datadir@
datarootdir = @datarootdir@
pkgdatadir=@pkgdatadir@
top_srcdir = @top_srcdir@
madinc = @madinc@
# this file is used by the subdirectories, so top_objdir is relative to those
top_objdir = ../../..

PWD = $(shell pwd)
RANLIB = @RANLIB@
YACC = @YACC@
CC = @CC@
CXX = @CXX@
FC = @FC@
MKDIRS = @MKDIRS@
INSTALL = @INSTALL@
INSTALL_INCLUDE = @INSTALL_INCLUDE@
INSTALL_DATA = @INSTALL_DATA@
INSTALL_PROGRAM = @INSTALL_PROGRAM@
MKDIRS = @MKDIRS@

PSICPPFLAGS = @PSICPPFLAGS@
CPPFLAGS = @CPPFLAGS@

PYTHON_INCLUDE = @PYTHON_INCLUDE@
BOOSTVER = @BOOSTVER@
BOOST_CPPFLAGS = @BOOST_CPPFLAGS@

CDEF = @CDEF@
COPT = @COPT@
COTH = @COTH@
CDBG = @CDBG@
CINC = @CINC@

CXXDEF = @CXXDEF@
CXXOPT = @CXXOPT@
CXXOTH = @CXXOTH@
CXXDBG = @CXXDBG@
CXXINC = @CXXINC@

FDEF = @FDEF@
FOPT = @FOPT@
FOTH = @FOTH@
FDBG = @FDBG@
FINC = @FINC@
FSTATIC = @FSTATIC@

# The suffix generated by the -M compiler option
CCDEPENDSUF = @CCDEPENDSUF@
CXXDEPENDSUF = @CXXDEPENDSUF@
CCDEPENDFLAGS = @CCDEPENDFLAGS@
CXXDEPENDFLAGS = @CXXDEPENDFLAGS@
CCDEPEND = $(CC)
CXXDEPEND = $(CXX)

# flags to be passed to compilers
CFLAGS = @CFLAGS@
CXXFLAGS = @CXXFLAGS@
FFLAGS = @FFLAGS@
FFLAGS_NOSTATIC = $(FOPT) $(FOTH) $(FDBG)

# set up suffixes needed for various files
F77SUF = @F77SUF@
OBJSUF = @OBJSUF@
LIBSUF = @LIBSUF@

#
# Initialize other variables.
#

AR = @AR@
ARFLAGS = @ARFLAGS@

LDFLAGS = @LDFLAGS@
