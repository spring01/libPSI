srcdir = @srcdir@
VPATH = @srcdir@

include ../MakeVars

PSITARGET=$(top_objdir)/lib/libPSI_mints_wrapper.a

CXXSRC = $(notdir $(wildcard $(srcdir)/*.cc))
DEPENDINCLUDE = $(notdir $(wildcard $(srcdir)/*.h))

BINOBJ = $(CXXSRC:%.cc=%.o)

include ../MakeRules

ifneq "$(MAKECMDGOALS)" "clean"
$(BINOBJ:%.o=%.d): $(DEPENDINCLUDE)
include $(BINOBJ:%.o=%.d)
endif

