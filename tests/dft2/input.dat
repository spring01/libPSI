#! DFT Functional Test

E11 = -74.9493412744 #TEST

memory 250 mb

molecule h2o {
0 1
O
H 1 1.0
H 1 1.0 2 104.5
}

set globals {
print 6
#debug 0
basis sto-3g
guess core
scf_type direct
dft_spherical_points 302
dft_radial_points 100
dft_max_points 400
dft_basis_tol 1.0E-10
reference rks
}

set dft_functional b_x
V11 = energy('scf')
compare_values(E11,V11, 3, "RKS  0 1   B97 Energy") #TEST

set dft_boxing_scheme octree
V11 = energy('scf')
compare_values(E11,V11, 3, "RKS  0 1   B97 Energy") #TEST
