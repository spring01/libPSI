	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-5.in $(TARGET) $(PSIAUTOTEST) FREQ-5.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-6.in $(TARGET) $(PSIAUTOTEST) FREQ-6.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-7.in $(TARGET) $(PSIAUTOTEST) FREQ-7.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-8.in $(TARGET) $(PSIAUTOTEST) FREQ-8.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-9.in $(TARGET) $(PSIAUTOTEST) FREQ-9.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-10.in $(TARGET) $(PSIAUTOTEST) FREQ-10.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-11.in $(TARGET) $(PSIAUTOTEST) FREQ-11.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-12.in $(TARGET) $(PSIAUTOTEST) FREQ-12.out
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-13.in $(TARGET) $(PSIAUTOTEST) FREQ-13.out
	/bin/cat $(SRCDIR)/tests >> FREQ-master.in
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) $(PERL) ../runtest.pl FREQ-master.in $(TARGET) $(PSIAUTOTEST) FREQ-master.out
	mv $(TARGET) $(PASSFILE)

clean:
	@SRCDIR=$(SRCDIR) PSIDATADIR=$(SRC_PSIDATADIR) 
	/bin/rm -f $(TARGET) $(PASSFILE) $(GARBAGE) FREQ-*

