#! RHF cc-pVQZ energy for the BH molecule, with Cartesian input.

memory 500 mb


molecule bh {
    b      0.0000        0.0000        0.0000
    h      0.0000        0.0000        1.0000
}

set globals = {
    scf_type   direct
    basis      cc-pVQZ
    e_convergence 10
}

thisenergy = energy('scf')

